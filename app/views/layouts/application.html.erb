<!DOCTYPE html>
<html>
<head>
  <title>BushwoodCC</title>
  
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
<link href='http://fonts.googleapis.com/css?family=Graduate|Open+Sans' rel='stylesheet' type='text/css'>

    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
  <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.11.0/jquery-ui.min.js"></script>
  <%= javascript_tag "var AUTH_TOKEN = '#{form_authenticity_token}';" if protect_against_forgery? %>
  <%= favicon_link_tag 'favicon.ico' %>
</head>

<body>
  <% if user_signed_in?%>
    <%= render "layouts/nav" %>
  <%end%>
  <div class="errors">
    <%if notice%>
    <div class="alert alert-info yellow-alerts">
      <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
      <%= notice %></div>
    <%elsif alert%>
    <div class="alert alert-danger red-alerts"><button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button><%= alert %></div>
    <%end%>
  </div>
  
  <div class="main">
  <%= yield %>
  <end>

</body>
</html>
